{
    "name": "AccountTab",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Account Tab",
    "short_name": "AccountTab",  
    "icons": {
        "16": "./img/icon16.png",
        "48": "./img/icon48.png",
        "128": "./img/icon128.png"
        
    },
    "background": {
        "scripts": ["./js/bundle.js"]
        
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs"
    ]    
}