{
    "name": "AccountTab",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Account Tab",
    "short_name": "AccountTab",  
    "icons": {
        "16": "./img/icon16.png",
        "48": "./img/icon48.png",
        "128": "./img/icon128.png"
        
    },
    "background": {
        "scripts": ["./js/bundle.js"]
        
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs"
    ],    
    "content_security_policy": "script-src 'self' https://www.gstatic.com https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'"
}